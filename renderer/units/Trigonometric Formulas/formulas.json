        [
            "\\sin x = \\frac 1 {\\csc x}",
            "\\cos x = \\frac 1 {\\sec x}",
            "\\tan x = \\frac {\\sin x} {\\cos x} = \\frac 1 {\\cot x}",
            "\\cot x = \\frac {\\cos x} {\\sin x} = \\frac 1 {\\tan x}",
            "\\sec x = \\frac 1 {\\cos x}",
            "\\csc x = \\frac 1 {\\sin x}"
        ]
    },
    {
        "category": "Symmetry Identities",
        "items":
        [
            "\\sin (-x) = - \\sin x",
            "\\cos (-x) = \\cos x",
            "\\tan (-x) = - \\tan x",
            "\\cot (-x) = - \\cot x",
            "\\sec (-x) = \\sec x",
            "\\csc (-x) = - \\csc x"
        ]
    },
    {
        "category": "Cofunction Identities",
